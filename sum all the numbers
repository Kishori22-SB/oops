package mysum;

public class Sum {
 
	    int calc(int start, int end) {
	        int sum = 0;
	        for (int i = start; i <= end; i++) sum += i;
	        return sum;
	    
	}

	

}
package mysum;

public class Main {
 
	    public static void main(String[] args) {
	        Sum s = new Sum();
	        System.out.println("Sum from 1 to 50: " + s.calc(1, 50));
	    }
	}


